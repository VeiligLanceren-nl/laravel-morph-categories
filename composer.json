{
    "name": "veiliglanceren/laravel-morph-categories",
    "description": "A Laravel package to support polymorphic category relationships for any model.",
    "keywords": ["laravel", "category", "morph", "polymorphic", "taxonomy"],
    "homepage": "https://github.com/veiliglanceren/laravel-morph-category",
    "support": {
        "issues": "https://github.com/veiliglanceren/laravel-morph-category/issues",
        "source": "https://github.com/veiliglanceren/laravel-morph-category"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.1.0",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "spatie/laravel-sluggable": "^2.0|^3.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "ext-dom": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^2.0 || ^3.0",
        "pestphp/pest-plugin-laravel": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "VeiligLanceren\\LaravelMorphCategories\\": "src/"
        },
        "files": [
            "src/Support/Helpers/models.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Niels Hamelink"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VeiligLanceren\\LaravelMorphCategories\\CategoryServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
